{
  "name": "rdsystem",
  "version": "0.0.0-lerna",
  "private": true,
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js ",
    "test": "jest",
    "libs": "babel --config-file ./.babelrc libs --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/*.spec.tsx\"",
    "types": "tsc -p tsconfig.lib.json",
    "copy": "node ./scripts/copy-files.js",
    "build:libs": "run-s libs types copy",
    "lint": "eslint ./ --fix",
    "format": "prettier --write ./",
    "commit": "git-cz",
    "version": "lerna version --force-publish"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^27.0.2",
    "@types/material-ui": "^0.21.9",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "^8.1.0",
    "base-href-webpack-plugin": "^3.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^5.2.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "jest-environment-jsdom": "^27.2.0",
    "lerna": "^4.0.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.3",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint --edit"
    }
  }
}
